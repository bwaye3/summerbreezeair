{#
/**
 * @file
 * Gavias's theme implementation to display a single Drupal page.
 */
#}

<div class="body-page gva-body-page">
  {% include header_skin %}

  {% if page.breadcrumbs %}
    <div class="breadcrumbs mt-3">
      <div class="col text-center credit-card-formmt-4">

        <div class="text-center">
          <img src="/sites/default/files/inline-images/handy-dandy.png" class="handy-dan" alt="handy dan the fix it man">
        </div>
        <h4><em>Let's fix this!</em></h4>
        <h2>Something went wrong</h2>

        <div class="my-5 pb-5 content block-content">
          <form class="search-form" data-drupal-selector="search-form" action="../search/node" method="post"
                id="search-form" accept-charset="UTF-8">
            <div class="container-inline js-form-wrapper form-wrapper" data-drupal-selector="edit-basic"
                 id="edit-basic">
              <div class="js-form-item form-item js-form-type-search form-item-keys js-form-item-keys">
                <input data-drupal-selector="edit-keys" type="search" id="edit-keys" name="keys" value="" size="30"
                       maxlength="255" class="form-search">
              </div>
              <input class="search-form__submit button js-form-submit form-submit" data-drupal-selector="edit-submit"
                     type="submit" id="edit-submit" name="op" value="Search">
            </div>
            <input autocomplete="off" data-drupal-selector="form-yeewaly9tw-csclgvpivyxho1mqaq3sm-q47nq4ali4"
                   type="hidden" name="form_build_id" value="form-YEewAly9TW_cSClGVPIVyxHO1mQaQ3sm_Q47nQ4ali4">
            <input data-drupal-selector="edit-search-form" type="hidden" name="form_id" value="search_form">
          </form>
        </div>
      </div>
    </div>
  {% endif %}

  <div role="main" class="main main-page">

    <div class="clearfix"></div>

    <div id="content" class="content content-full">
      <div class="container">
      </div>
    </div>



  </div>

</div>


{% include directory ~ '/templates/page/footer.html.twig' %}
